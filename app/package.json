{
  "name": "devRant.io",
  "productName": "devRant.io",
  "version": "1.0.1",
  "description": "The unofficial electron app for devRant.io",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "clean": "cleanup",
    "postinstall": "install-app-deps",
    "package": "build",
    "build-win": "electron-packager . --overwrite --prune --out=./dist --ignore='node_modules' --platform=win32 --arch=x64 --icon=../icon/favicon.ico --version-string.FileDescription=devRant.io",
    "build-osx": "electron-packager . --overwrite --prune --out=./dist --ignore='node_modules' --platform=darwin --arch=x64 --icon=../icon/favicon.icns"
  },
  "build": {
    "app-bundle-id": "uk.meadowcottage.devRant-io",
    "app-category-type": "public.app-category.social-networking",
    "iconUrl": "https://raw.githubusercontent.com/Meadowcottage/Devrant.io/master/icon/favicon.ico",
    "win": {
      "loadingGif": "build/loadingGif.gif",
      "remoteReleases": "https://github.com/Meadowcottage/Devrant.io"
    }
  },
  "repository": {
    "url": "https://github.com/meadowcottage/devrant.io.git"
  },
  "keywords": [
    "electron",
    "devrant.io"
  ],
  "author": "[MEADOW_DEV]",
  "license": "GNU General Public License",
  "bugs": {
    "url": "https://github.com/meadowcottage/devrant.io/issues/"
  },
  "homepage": "https://github.com/meadowcottage/devrant.io/",
  "devDependencies": {
    "electron-builder": "^5.3.0",
    "electron-prebuilt": "^1.2.2",
    "electron-winstaller": "^2.3.1"
  }
}
